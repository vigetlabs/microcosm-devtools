{
  "name": "microcosm-devtools",
  "version": "0.0.0",
  "description": "Devtools for Microcosm",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config shells/dev/webpack.config.js",
    "dev:chrome": "webpack --watch --hide-modules --config shells/chrome/webpack.config.js",
    "lint": "./bin/lint",
    "format": "./bin/format",
    "build": "./bin/build",
    "test": "echo WRITE TESTS; exit 0",
    "zip": "cd shells && zip -r -FS ../dist/chrome.zip chrome -x *src/* -x *webpack.config.js",
    "release": "npm run test && npm run build && npm run zip"
  },
  "keywords": [
    "microcosm",
    "devtools"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "prettier": "^1.5.2",
    "react-hot-loader": "3.0.0-beta.6",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.0",
    "circular-json-es6": "^2.0.1",
    "classnames": "^2.2.5",
    "d3-scale": "^1.0.6",
    "microcosm": "^12.9.0",
    "paths-js": "^0.4.5",
    "react": "^16.0.0-beta.2",
    "react-dom": "^16.0.0-beta.2",
    "react-inspector": "^2.0.0",
    "react-json-tree": "^0.10.9"
  }
}
